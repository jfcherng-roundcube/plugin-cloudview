{
    "name": "jfcherng-roundcube/cloudview",
    "description": "A Roundcube plugin which lets you view documents with cloud viewer.",
    "type": "roundcube-plugin",
    "license": "MIT",
    "homepage": "https://github.com/jfcherng-roundcube/plugin-cloudview",
    "version": "0.4.0",
    "authors": [
        {
            "name": "Jack Cherng",
            "email": "jfcherng@gmail.com"
        },
        {
            "name": "Andrian Galkin",
            "email": "galandrix@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "roundcube/plugin-installer": "~0.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16"
    },
    "replace": {
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "fix": [
            "php-cs-fixer fix --verbose"
        ]
    }
}
